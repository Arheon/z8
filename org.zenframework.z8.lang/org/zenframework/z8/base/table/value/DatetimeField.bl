[native "org.zenframework.z8.server.base.table.value.DatetimeField"]

class DatetimeField extends Field {
	stretch = false;

	format = "d/m/Y H:i:s"; // http://php.net/manual/ru/function.date.php

	virtual public datetime getDefault();	// default datetime.MIN_VALUE 
	public void setDefault(datetime defaultValue);

	public operator sql_datetime();

	virtual public datetime get();
	
	public DatetimeField operator = (datetime value);
}